{
    "name": "pocketframe/installer",
    "type": "library",
    "license": "MIT",
    "require": {
        "symfony/console": "^7.2",
        "symfony/filesystem": "^7.2",
        "symfony/yaml": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "symfony/http-client": "^7.2",
        "symfony/browser-kit": "^7.2",
        "symfony/process": "^7.2",
        "mockery/mockery": "^1.6"
    },
    "authors": [
        {
            "name": "William Asaba",
            "email": "asabawilliamdk@yahoo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PocketFrame\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PocketFrame\\Tests\\": "src/tests/"
        }
    },
    "bin": ["pocketframe"],
    "minimum-stability": "stable",
    "prefer-stable": true
}
